[
    {
        "id": 1,
        "name": "Spawn Monster",
        "command": "/monster <monster_id> <amount> <level>",
        "args": [
            {
                "key": "monster_id",
                "name": "Monster ID",
                "description": "This command will spawn a monster in your world with a custom amount.",
                "type": "search",
                "api": {
                    "game": "gi",
                    "jsonBody": {
                        "command": false,
                        "search": [
                            "monster_id"
                        ],
                        "category": [
                            "Monsters"
                        ],
                        "limit": 10
                    }
                }
            },
            {
                "key": "amount",
                "name": "Amount",
                "description": "Amount of monsters to spawn.",
                "type": "number"
            },
            {
                "key": "level",
                "name": "Level",
                "description": "Level of the monster.",
                "type": "number",
                "limit": {
                    "min": 1,
                    "max": 200
                }
            }
        ]
    },
